#: approver/phoneappapprover.cpp:275
msgid "Accept"
msgstr "קבלה"

#: approver/phoneappapprover.cpp:252
msgid "Caller number is not available"
msgstr "מספר המען אינו זמין"

#: approver/phoneappapprover.cpp:249
#, qt-format
msgid "Calling from %1"
msgstr "שיחה מ%1"

